{
	"compilerOptions": {
		"target": "es2020",
		"lib": ["es2020", "DOM"],
		"module": "es2020",
		"sourceMap": false,
		"outDir": "../lib/",
		"declaration": false,
		"declarationMap": false,
		"moduleResolution": "node",
		"jsxFactory": "Snowblind.make",
		"jsxFragmentFactory": "Snowblind.Fragment",
		"allowSyntheticDefaultImports": true,
		"jsx": "react",
	},
	"compileOnSave": true,
	"exclude": ["node_modules"]
}